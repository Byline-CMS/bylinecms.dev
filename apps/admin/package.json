{
  "name": "@byline/admin",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:vite": "vite",
    "dev:api": "tsx --env-file=.env --watch api/server.ts",
    "build": "run-s build:*",
    "build:client": "tsc -p tsconfig.json && vite build",
    "build:api": "tsc -p tsconfig.server.json",
    "byline:generate": "tsx byline/scripts/generate.ts",
    "drizzle:generate": "tsc -p tsconfig.json && drizzle-kit generate --config=drizzle.config.ts",
    "drizzle:push": "tsc -p tsconfig.json && drizzle-kit push --config=drizzle.config.ts",
    "drizzle:migrate": "drizzle-kit migrate --config=drizzle.config.ts",
    "drizzle:drop": "drizzle-kit drop --config=drizzle.config.ts",
    "drizzle:seed": "tsx --no-warnings --env-file=.env ./database/seeds/index.ts",
    "clean": "rimraf node_modules dist build .turbo "
  },
  "dependencies": {
    "@byline/shared": "workspace:*",
    "@byline/uikit": "workspace:*",
    "@fastify/cors": "^11.0.1",
    "@lexical/clipboard": "0.30.0",
    "@lexical/code": "0.30.0",
    "@lexical/hashtag": "0.30.0",
    "@lexical/headless": "0.30.0",
    "@lexical/html": "0.30.0",
    "@lexical/link": "0.30.0",
    "@lexical/list": "0.30.0",
    "@lexical/mark": "0.30.0",
    "@lexical/markdown": "0.30.0",
    "@lexical/overflow": "0.30.0",
    "@lexical/react": "0.30.0",
    "@lexical/rich-text": "0.30.0",
    "@lexical/selection": "0.30.0",
    "@lexical/table": "0.30.0",
    "@lexical/utils": "0.30.0",
    "@tanstack/react-query": "^5.80.10",
    "@tanstack/react-router": "^1.121.27",
    "@tanstack/react-router-devtools": "^1.121.27",
    "fastify": "^5.4.0",
    "intl-messageformat": "^10.7.16",
    "jose": "^6.0.11",
    "lexical": "0.30.0",
    "npm-run-all": "^4.1.5",
    "pg": "^8.16.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-error-boundary": "^6.0.0",
    "react-hook-form": "^7.58.1",
    "uuid": "^11.1.0",
    "zod": "^3.25.67",
    "zod-form-data": "^3.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.0.2",
    "@tailwindcss/vite": "^4.1.10",
    "@tanstack/router-plugin": "^1.121.29",
    "@types/node": "^24.0.3",
    "@types/nodemailer": "^6.4.17",
    "@types/nodemailer-html-to-text": "^3.1.3",
    "@types/pg": "^8.15.4",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.5.2",
    "classnames": "^2.5.1",
    "drizzle-kit": "^0.31.1",
    "drizzle-orm": "^0.44.2",
    "tailwindcss": "4.1.10",
    "tsx": "^4.20.3",
    "typescript": "5.8.3",
    "vite": "^6.3.5"
  }
}