{
  "$schema": "https://turborepo.org/schema.json",
  "ui": "stream",
  "tasks": {
    "build": {
      "cache": false,
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        "build/**"
      ]
    },
    "dev": {
      "cache": false,
      "dependsOn": [
        "^dev"
      ],
      "outputs": [],
      "env": [
        "NODE_ENV"
      ],
      "persistent": true
    },
    "db:init": {
      "cache": false,
      "persistent": true,
      "interactive": true
    },
    "drizzle:generate": {
      "cache": false
    },
    "drizzle:migrate": {
      "cache": false
    },
    "dev:seed": {
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "typecheck": {
      "outputs": []
    },
    "test": {
      "outputs": []
    },
    "start": {
      "cache": false,
      "dependsOn": [
        "^start"
      ]
    }
  },
  "globalEnv": []
}